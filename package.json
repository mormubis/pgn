{
  "author": "Adrian de la Rosa <adrian@delarosab.me>",
  "dependencies": {
    "nearley": "^2.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "gitmoji-cli": "^3.2.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-cli": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "rollup": "^2.4.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-visualizer": "^4.0.4"
  },
  "license": "MIT",
  "main": "index.js",
  "name": "@echecs/pgn",
  "repository": "mormubis/pgn",
  "scripts": {
    "build": "rollup -c",
    "build:ci": "npm build",
    "compile": "nearleyc src/grammar.ne -o src/grammar.js",
    "coverage": "jest --coverage --no-cache --silent",
    "coverage:ci": "npm run coverage -- --maxWorkers 4",
    "format": "npm run format:ci -- --write",
    "format:ci": "prettier --ignore-path .gitignore -l \"**/*.+(js|json|md|yml)\"",
    "lint": "eslint \"**/*.js\" --fix",
    "lint:ci": "npm run lint",
    "prestart": "npm run compile",
    "start": "rollup -c -w",
    "test": "jest",
    "test:ci": "npm test -- --maxWorkers 4 --no-cache --silent",
    "test:grammar": "nearley-test -s DATABASE src/grammar.js < ./test.pgn",
    "test:watch": "jest --watch"
  },
  "version": "1.0.0"
}
